<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="ztablejson.Products" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Title">
		<content>
	    	<Table items="{/d/results}">
	    	   <columns>
	    	      <Column>
	    	         <header>
	    	           <Text text="Product ID"/>
	    	         </header>
	    	      </Column>
	    	      <Column>
	    	         <header>
	    	           <Text text="Name"/>
	    	         </header>
	    	      </Column>
	    	      <Column>
	    	         <header>
	    	           <Text text="Description"/>
	    	         </header>
	    	      </Column>
	    	      <Column>
	    	         <header>
	    	           <Text text="Price"/>
	    	         </header>
	    	      </Column>
	    	   </columns>
	    	   <items>
	    	     <ColumnListItem>
	    	        <cells>
	    	          <Text text="{ProductID}"/>
	    	          <Input value="{Name}"/>
	    	          <Text text="{Decription}"/>
	    	          <Text text="{Price}"/>
	    	        </cells>
	    	     </ColumnListItem>
	    	   </items>
	    	</Table>
		</content>
	</Page>
</core:View>